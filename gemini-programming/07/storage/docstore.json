e": "redhood4.txt", "file_type": "text/plain", "file_size": 744, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}}, "42b98075-236c-4dbe-96c1-0ebb8b41b2a1": {"node_ids": ["ef8c5368-f586-42a2-ab92-67a88c251275"], "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood5.txt", "file_name": "redhood5.txt", "file_type": "text/plain", "file_size": 820, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}}, "2a4a8515-d3e4-463c-8d02-349515419556": {"node_ids": ["15062776-e7d1-490c-81d7-ecf9fb8cf36d"], "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood6.txt", "file_name": "redhood6.txt", "file_type": "text/plain", "file_size": 786, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}}, "a9b6893e-ca7c-4478-a53b-9de79fc711b2": {"node_ids": ["fab2166c-c9f4-4954-bb42-cad8ee7a2a15"], "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood7.txt", "file_name": "redhood7.txt", "file_type": "text/plain", "file_size": 807, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}}, "8a446e75-beb4-4d83-a0b8-91fb93bd85d8": {"node_ids": ["b4d93ac5-d3d5-40d4-837f-b9b03ac2d1bf", "45a7f998-bdc1-47b9-b9c6-fdac842a2d96", "e694d538-593d-4a51-8bc3-a0a2cde1958d"], "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}}}, "docstore/data": {"ad7b6932-7351-4591-bc80-b49d54f75a3a": {"__data__": {"id_": "ad7b6932-7351-4591-bc80-b49d54f75a3a", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood1.txt", "file_name": "redhood1.txt", "file_type": "text/plain", "file_size": 854, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "8c379d93-7550-4bf0-9efa-3773aa8b9168", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood1.txt", "file_name": "redhood1.txt", "file_type": "text/plain", "file_size": 854, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "360eee356f1dc2796ea26dc573d75a0c3a4e1ab069f3302c9fcc0a049bd04a2e", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "2099\ub144 \ub124\uc624-\uc11c\uc6b8, \uc0b0\uc131\ube44\uac00 \ub04a\uc784\uc5c6\uc774 \ub0b4\ub9ac\ub294 \ud558\uce35 \uad6c\uc5ed. \uc815\ubcf4 \ubc30\ub2ec\ubd80 '\uc740\ube44'\ub294 \ub0a1\uc740 \uc544\ud30c\ud2b8 \uc625\uc0c1\uc5d0\uc11c \uc758\ub8b0\uc778\uc744 \ub9cc\ub0ac\ub2e4. \uadf8\ub140\uc758 \ud2b8\ub808\uc774\ub4dc\ub9c8\ud06c\ub294 \uad11\ud559 \ubbf8\ucc44 \uae30\ub2a5\uc774 \ud0d1\uc7ac\ub41c \ubd89\uc740\uc0c9 \ud640\ub85c\uadf8\ub7a8 \ud6c4\ub4dc\uc600\ub2e4.\n\n\"\uc774\uac78 '\ud560\uba38\ub2c8'\uc5d0\uac8c \uc804\ud574\ub77c.\"\n\uc758\ub8b0\uc778\uc740 \ub5a8\ub9ac\ub294 \uc190\uc73c\ub85c \ubd89\uac8c \ube5b\ub098\ub294 \uc0dd\uccb4 \ub370\uc774\ud130 \uce69\uc744 \uac74\ub138\ub2e4. \uadf8 \uc548\uc5d0\ub294 \uba54\uac00\ucf54\ud504(\uac70\ub300 \uae30\uc5c5) '\uc6b8\ud504 \uc0ac'\uc758 \ubd88\ubc95 \uc0dd\uccb4 \uc2e4\ud5d8 \uc99d\uac70\uac00 \ub4e4\uc5b4 \uc788\uc5c8\ub2e4. '\ud560\uba38\ub2c8'\ub294 \uad6c\uc2dc\uac00\uc9c0 \uc9c0\ud558 \ubc99\ucee4\uc5d0 \uc228\uc5b4 \uc9c0\ub0b4\ub294 \uc804\uc124\uc801\uc778 \ud574\ucee4\uc758 \ucf54\ub4dc\ub124\uc784\uc774\uc5c8\ub2e4.\n\n\"\uacbd\ub85c\ub97c \uc870\uc2ec\ud574. \ub188\ub4e4\uc758 '\uc0ac\ub0e5\uac1c'\ub4e4\uc774 \ub0c4\uc0c8\ub97c \ub9e1\uc558\uc5b4.\"\n\uc740\ube44\ub294 \ubd89\uc740 \ud6c4\ub4dc\ub97c \uba38\ub9ac \uc704\ub85c \ub20c\ub7ec\uc4f0\uba70 \ube57\uc18d\uc73c\ub85c \uc0ac\ub77c\uc84c\ub2e4. \uadf8\ub140\uc758 \ub9dd\ub9c9 \ub514\uc2a4\ud50c\ub808\uc774\uc5d0 \ubaa9\uc801\uc9c0\uae4c\uc9c0\uc758 \uacbd\ub85c\uac00 \ubd89\uc740 \uc2e4\uc120\uc73c\ub85c \ud45c\uc2dc\ub418\uc5c8\ub2e4.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 360, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "6c21d2ee-e785-4ab2-9914-1d6376554885": {"__data__": {"id_": "6c21d2ee-e785-4ab2-9914-1d6376554885", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood2.txt", "file_name": "redhood2.txt", "file_type": "text/plain", "file_size": 695, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "64b1dcae-b09d-4368-8a31-084928b58b19", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood2.txt", "file_name": "redhood2.txt", "file_type": "text/plain", "file_size": 695, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "b6d1c9674248bf36a63c8ec5cbb28627b9688a7bd5a3c67b6c53762c0099a4fe", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\ub3c4\uc2dc\uc758 \ub4b7\uace8\ubaa9\uc740 \uac70\ub300\ud55c \ub124\uc628\uc0ac\uc778\uc774 \ub9cc\ub4e4\uc5b4\ub0b8 \uc778\uacf5\uc801\uc778 \uc232\uc774\uc5c8\ub2e4. \uc740\ube44\ub294 \ub4dc\ub860 \uac10\uc2dc\ub9dd\uc744 \ud53c\ud574 \uc2ac\ub7fc\uac00\uc758 \ubcf5\uc7a1\ud55c \ud30c\uc774\ud504\ub77c\uc778 \uc704\ub97c \ub2ec\ub838\ub2e4. \ubc14\ub2e5\uc740 \uc816\uc5b4 \uc788\uc5c8\uace0, \uacf5\uae30\ub294 \ub9e4\uce90\ud55c \uc804\uc790 \ub2f4\ubc30 \uc5f0\uae30\uc640 \uc624\uc874 \ub0c4\uc0c8\ub85c \uac00\ub4dd\ud588\ub2e4.\n\n\uadf8\ub54c, \uc740\ube44\uc758 \uccad\uac01 \uc13c\uc11c\uc5d0 \ubbf8\uc138\ud55c \ub178\uc774\uc988\uac00 \uc7a1\ud614\ub2e4. \uc77c\ubc18\uc801\uc778 \uae30\uacc4 \uc18c\uc74c\uc774 \uc544\ub2c8\uc5c8\ub2e4. \ub204\uad70\uac00 \uace0\uc131\ub2a5 \uc2a4\ud154\uc2a4 \ubaa8\ub4c8\uc744 \ucf1c\uace0 \uadf8\ub140\ub97c \ucd94\uc801\ud558\uace0 \uc788\uc5c8\ub2e4. \uc740\ube44\ub294 \ubcf8\ub2a5\uc801\uc73c\ub85c \uacbd\ub85c\ub97c \uc218\uc815\ud574 \ub354 \uae4a\uace0 \uc5b4\ub450\uc6b4 '\ud3d0\uae30\ubb3c \ucc98\ub9ac \uad6c\uc5ed'\uc73c\ub85c \ubc29\ud5a5\uc744 \ud2c0\uc5c8\ub2e4. \uc232\uc18d\uc758 \uc624\uc194\uae38 \ub300\uc2e0, \uadf8\ub140\ub294 \ub370\uc774\ud130 \uc4f0\ub808\uae30\uac00 \ud750\ub974\ub294 \ud558\uc218\ub3c4\ub97c \ud0dd\ud588\ub2e4.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 285, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "711d038c-98c7-4e86-a34e-be46313f396c": {"__data__": {"id_": "711d038c-98c7-4e86-a34e-be46313f396c", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood3.txt", "file_name": "redhood3.txt", "file_type": "text/plain", "file_size": 746, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "a9e8535b-cf4b-4f9d-90d0-60cfae769efd", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood3.txt", "file_name": "redhood3.txt", "file_type": "text/plain", "file_size": 746, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "3ec5b3b2e3093fde5732b6860d6f95c6d2fc065e93dab35b1428022115f43a54", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\ud3d0\uae30\ubb3c \uad6c\uc5ed\uc758 \ucd9c\uad6c, \ud55c \ub0a8\uc790\uac00 \uc740\ube44\uc758 \uae38\uc744 \ub9c9\uc544\uc130\ub2e4. \ud68c\uc0c9 \ud2b8\ub80c\uce58\ucf54\ud2b8\ub97c \uc785\uc740 \uadf8\ub294 \uac89\ubcf4\uae30\uc5d4 \ud3c9\ubc94\ud574 \ubcf4\uc600\uc9c0\ub9cc, \uc740\ube44\uc758 \uc2a4\uce90\ub108\ub294 \uadf8\uc758 \ubab8 90%\uac00 \uad70\uc6a9 \ud06c\ub86c \uc758\uccb4\uc784\uc744 \uc54c\ub9ac\uace0 \uc788\uc5c8\ub2e4. \ucf54\ub4dc\ub124\uc784 '\uc6b8\ud504'. \uc6b8\ud504 \uc0ac\uc758 \ucd5c\uace0 \ud574\uacb0\uc0ac\uc600\ub2e4.\n\"\uaf2c\ub9c8 \uc544\uac00\uc528, \ubb34\uac70\uc6b4 \uc9d0\uc744 \ub4e4\uace0 \uac00\ub294\uad70. \ub0b4\uac00 \ub3c4\uc640\uc904\uae4c?\"\n\ub0a8\uc790\uc758 \ubaa9\uc18c\ub9ac\ub294 \uae01\ud788\ub294 \ub4ef\ud55c \uae30\uacc4\uc74c\uc774 \uc11e\uc5ec \uc788\uc5c8\ub2e4. \uadf8\ub294 \uc740\ube44\uac00 \uac00\uc9c4 \uce69\uc774 \ubb34\uc5c7\uc778\uc9c0 \uc815\ud655\ud788 \uc54c\uace0 \uc788\uc5c8\ub2e4.\n\"\uc9c0\ub984\uae38\uc744 \uc54c\ub824\uc8fc\uc9c0. \uadf8 \uce69\ub9cc \ub118\uae34\ub2e4\uba74 \uc548\uc804\ud558\uac8c \uc9d1\uc5d0 \uac08 \uc218 \uc788\uc5b4.\"\n\n\uc740\ube44\ub294 \ub300\ub2f5 \ub300\uc2e0 \uc8fc\uba38\ub2c8 \uc18d\uc758 EMP \uc218\ub958\ud0c4\uc744 \uaf49 \uc950\uc5c8\ub2e4. \"\ub09c \ub0af\uc120 \uc0ac\ub78c\uacfc\ub294 \ub370\uc774\ud130 \ud1b5\uc2e0\uc744 \ud558\uc9c0 \uc54a\uc544\uc694.\"", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 321, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "c6312275-9786-4a2f-8ea2-cc76c9e3f4e8": {"__data__": {"id_": "c6312275-9786-4a2f-8ea2-cc76c9e3f4e8", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood4.txt", "file_name": "redhood4.txt", "file_type": "text/plain", "file_size": 744, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "017ae22e-5a8e-40c6-8392-db6bb95721e1", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood4.txt", "file_name": "redhood4.txt", "file_type": "text/plain", "file_size": 744, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "98c7c7639379b8e62277f34e1664a96ea0294474715ba9d168de04a1c405ff2a", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\ud3ed\ubc1c\uc74c\uacfc \ud568\uaed8 \uc740\ube44\ub294 \uc9c0\uc0c1\uc73c\ub85c \ud280\uc5b4 \uc62c\ub790\ub2e4. EMP\ub294 \uc6b8\ud504\ub97c \uc7a0\uc2dc \ub9c8\ube44\uc2dc\ucf30\uc9c0\ub9cc, \uadf8\ub294 \uae08\uc138 \uc2dc\uc2a4\ud15c\uc744 \uc7ac\ubd80\ud305\ud558\uace0 \uc9d0\uc2b9 \uac19\uc740 \uc18d\ub3c4\ub85c \ucd94\uaca9\uc744 \uc2dc\uc791\ud588\ub2e4. \uadf8\uc758 \ud314\uc774 \ubcc0\ud615\ub418\uc5b4 \ub0a0\uce74\ub85c\uc6b4 \ubaa8\ub178-\uc640\uc774\uc5b4 \ud074\ub85c(\ubc1c\ud1b1)\uac00 \ud280\uc5b4 \ub098\uc654\ub2e4.\n\n\uc740\ube44\ub294 \ub9c8\uadf8\ub124\ud2f1 \ubd80\uce20\ub97c \uc791\ub3d9\uc2dc\ucf1c \uac74\ubb3c\uc758 \uc218\uc9c1 \ubcbd\uc744 \ub2ec\ub838\ub2e4. \ubd89\uc740 \ud6c4\ub4dc\uc758 \uad11\ud559 \ubbf8\ucc44\ub97c \ud65c\uc131\ud654\ud574 \uc8fc\ubcc0\uc758 \ub124\uc628 \uc0ac\uc778\uacfc \ub3d9\ud654\ub418\ub824 \ud588\uc9c0\ub9cc, \uc6b8\ud504\uc758 \uc5f4 \uac10\uc9c0 \uc13c\uc11c\ub294 \uadf8\ub140\ub97c \ub193\uce58\uc9c0 \uc54a\uc558\ub2e4. \ub3c4\uc2dc\uc758 \ub9c8\ucc9c\ub8e8 \uc0ac\uc774\ub97c \uc624\uac00\ub294 \uc228 \ub9c9\ud788\ub294 \ucd94\uaca9\uc804\uc774 \ubc8c\uc5b4\uc84c\ub2e4. \uc740\ube44\ub294 \ud574\ud0b9 \ud234\uc744 \uc774\uc6a9\ud574 \uc2e0\ud638\ub4f1\uc744 \uc870\uc791\ud558\uace0 \uad11\uace0\ud310\uc744 \ub5a8\uc5b4\ub728\ub9ac\uba70 \uac04\uc2e0\ud788 \uc6b8\ud504\uc640\uc758 \uac70\ub9ac\ub97c \ubc8c\ub838\ub2e4.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 306, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "ef8c5368-f586-42a2-ab92-67a88c251275": {"__data__": {"id_": "ef8c5368-f586-42a2-ab92-67a88c251275", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood5.txt", "file_name": "redhood5.txt", "file_type": "text/plain", "file_size": 820, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "42b98075-236c-4dbe-96c1-0ebb8b41b2a1", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood5.txt", "file_name": "redhood5.txt", "file_type": "text/plain", "file_size": 820, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "83102797f33705ccabfb7da3763ef6884964b8dae4dd66486ff1bf191401c7e5", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\uad6c\uc2dc\uac00\uc9c0\uc758 \ud3d0\ud5c8, \uc78a\ud600\uc9c4 \uc9c0\ud558\ucca0\uc5ed \uae4a\uc740 \uacf3\uc5d0 '\ud560\uba38\ub2c8'\uc758 \ubc99\ucee4\uac00 \uc788\uc5c8\ub2e4. \uc740\ube44\ub294 \uc228\uc744 \ud5d0\ub5a1\uc774\uba70 \uc721\uc911\ud55c \uac15\ucca0 \ubb38\uc744 \ud574\ud0b9\ud574 \uc5f4\uc5c8\ub2e4.\n\"\ud560\uba38\ub2c8! \ub370\uc774\ud130 \uac00\uc838\uc654\uc5b4\uc694!\"\n\n\ud558\uc9c0\ub9cc \ubc99\ucee4 \uc548\uc740 \ucc28\uac11\uac8c \uc2dd\uc5b4 \uc788\uc5c8\ub2e4. \uc218\ub9ce\uc740 \uc11c\ubc84\uac00 \uc6c5\uc6c5\uac70\ub9ac\ub294 \uc18c\ub9ac \ub300\uc2e0, \uae30\uad34\ud55c \uc815\uc801\ub9cc\uc774 \uac10\ub3cc\uc558\ub2e4. \uc911\uc559 \uc81c\uc5b4 \uc758\uc790\uc5d0\ub294 \ub1cc\uc640 \uc11c\ubc84\uac00 \uc9c1\uc811 \uc5f0\uacb0\ub41c \ub178\ud30c\uac00 \uc549\uc544 \uc788\uc5c8\uc9c0\ub9cc, \uadf8\ub140\uc758 \ubaa8\ub2c8\ud130\uc5d0\ub294 \ubd89\uc740\uc0c9 'ACCESS DENIED(\uc811\uadfc \uac70\ubd80)' \uba54\uc2dc\uc9c0\ub9cc\uc774 \uae5c\ube61\uc774\uace0 \uc788\uc5c8\ub2e4.\n\n\"\ub2a6\uc5c8\uad6c\ub098, \uc544\uac00\uc528.\"\n\uc5b4\ub460 \uc18d\uc5d0\uc11c \ud68c\uc0c9 \ucf54\ud2b8\uc758 \ub0a8\uc790\uac00 \uac78\uc5b4 \ub098\uc654\ub2e4. \uadf8\ub294 \uc740\ube44\ubcf4\ub2e4 \uba3c\uc800 \ub3c4\ucc29\ud574 '\ud560\uba38\ub2c8'\uc758 \uc2dc\uc2a4\ud15c\uc744 \uc7a5\uc545\ud558\uace0, \uc790\uc2e0\uc758 \ubaa9\uc18c\ub9ac\ub97c \ud560\uba38\ub2c8\uc758 \uc74c\uc131 \ud569\uc131\uae30\ub85c \ubcc0\uc870\ud574 \uc740\ube44\ub97c \uc720\uc778\ud588\ub358 \uac83\uc774\ub2e4.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 352, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "15062776-e7d1-490c-81d7-ecf9fb8cf36d": {"__data__": {"id_": "15062776-e7d1-490c-81d7-ecf9fb8cf36d", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood6.txt", "file_name": "redhood6.txt", "file_type": "text/plain", "file_size": 786, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "2a4a8515-d3e4-463c-8d02-349515419556", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood6.txt", "file_name": "redhood6.txt", "file_type": "text/plain", "file_size": 786, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "dab1ab198d606500be6d65f3448a9188549ce3267346e80739a267b16fc4b861", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\"\uadf8 \uce69\uc744 \ub0b4\ub193\uc73c\uc2dc\uc9c0.\"\n\uc6b8\ud504\uac00 \ubc1c\ud1b1\uc744 \uc138\uc6b0\uba70 \ub2e4\uac00\uc654\ub2e4. \uc740\ube44\ub294 \ub9c9\ub2e4\ub978 \uacf3\uc5d0 \ubab0\ub838\ub2e4. \ubb3c\ub9ac\uc801\uc778 \ud798\uc73c\ub85c\ub294 \uadf8\ub97c \uc774\uae38 \uc218 \uc5c6\uc5c8\ub2e4. \uc740\ube44\ub294 \uce69\uc744 \uac74\ub124\uc8fc\ub294 \ucc99\ud558\uba70 \uc2ac\ub86f\uc5d0 \uce69\uc744 \uaf42\uc558\ub2e4.\n\n\"\uba4d\uccad\ud558\uad70.\" \uc6b8\ud504\uac00 \ube44\uc6c3\uc73c\uba70 \uce69\uc758 \ub370\uc774\ud130\ub97c \uc790\uc2e0\uc758 \uc11c\ubc84\ub85c \uc804\uc1a1\ud558\ub824 \ud588\ub2e4.\n\ud558\uc9c0\ub9cc \uadf8 \uc21c\uac04, \ubc99\ucee4\uc758 \ubaa8\ub4e0 \ud654\uba74\uc774 \ubd89\uac8c \ubcc0\ud588\ub2e4. \uc740\ube44\uac00 \uac00\uc838\uc628 \uac83\uc740 \ub2e8\uc21c\ud55c \uc99d\uac70 \ub370\uc774\ud130\uac00 \uc544\ub2c8\uc5c8\ub2e4. \uadf8\uac83\uc740 \ud560\uba38\ub2c8\uac00 \uc0ac\uc804\uc5d0 \uc2ec\uc5b4\ub454 \uc790\uc728 \ub300\uc751 \ubc14\uc774\ub7ec\uc2a4, '\ub354 \ud5cc\ud130(The Hunter)'\uc758 \uc2e4\ud589\ud0a4\uc600\ub2e4.\n\n\"\ubb50, \ubb50\uc9c0? \ub0b4 \uc2dc\uc2a4\ud15c\uc774...!\"\n\uc6b8\ud504\uc758 \uc758\uccb4\uac00 \uacbd\ub828\uc744 \uc77c\uc73c\ucf30\ub2e4. \ubc14\uc774\ub7ec\uc2a4\ub294 \uc678\ubd80 \uc11c\ubc84\uac00 \uc544\ub2c8\ub77c, \uc811\uc18d\ub41c \uc6b8\ud504\uc758 \ub1cc \uc2e0\uacbd\ub9dd\uc744 \uc9c1\uc811 \uacf5\uaca9\ud558\uae30 \uc2dc\uc791\ud588\ub2e4.", "mimetype": "text/plain", "start_char_idx": 1, "end_char_idx": 342, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "fab2166c-c9f4-4954-bb42-cad8ee7a2a15": {"__data__": {"id_": "fab2166c-c9f4-4954-bb42-cad8ee7a2a15", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood7.txt", "file_name": "redhood7.txt", "file_type": "text/plain", "file_size": 807, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "a9b6893e-ca7c-4478-a53b-9de79fc711b2", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/redhood7.txt", "file_name": "redhood7.txt", "file_type": "text/plain", "file_size": 807, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "c446b3639d23a9616bd6510888abd60317021102d3c7dbac676fd569c2bbb343", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\uc6b8\ud504\ub294 \uacfc\ubd80\ud558\ub85c \uc778\ud574 \uc5f0\uae30\ub97c \ub0b4\ubfdc\uc73c\uba70 \uc4f0\ub7ec\uc84c\ub2e4. \uadf8\uc758 \uc2dc\uc57c\uac00 \uaebc\uc9c0\uae30 \uc804, \ub9c8\uc9c0\ub9c9\uc73c\ub85c \ubcf8 \uac83\uc740 \ubd89\uc740 \ud6c4\ub4dc\ub97c \uc4f4 \uc740\ube44\uac00 \ucc28\uac11\uac8c \uadf8\ub97c \ub0b4\ub824\ub2e4\ubcf4\ub294 \ubaa8\uc2b5\uc774\uc5c8\ub2e4.\n\n\uc740\ube44\ub294 \ud560\uba38\ub2c8\uc758 \uba54\uc778 \uc11c\ubc84\ub97c \ubcf5\uad6c\ud558\uace0, \uae30\uc5c5\uc758 \ube44\ub9ac \ub370\uc774\ud130\ub97c \uc804 \ub3c4\uc2dc\uc758 \ud640\ub85c\uadf8\ub7a8 \uad11\uace0\ud310\uc73c\ub85c \uc1a1\ucd9c\ud588\ub2e4. \ub124\uc624-\uc11c\uc6b8\uc758 \ubc24\ud558\ub298\uc774 \uc9c4\uc2e4\uc744 \uc54c\ub9ac\ub294 \ubd89\uc740 \ube5b\uc73c\ub85c \ubb3c\ub4e4\uc5c8\ub2e4.\n\n\"\uc218\uace0\ud588\ub2e4, \uc740\ube44\uc57c.\" \uc2a4\ud53c\ucee4\ub97c \ud1b5\ud574 \ubcf5\uad6c\ub41c \ud560\uba38\ub2c8\uc758 AI \uc74c\uc131\uc774 \ud758\ub7ec\ub098\uc654\ub2e4.\n\uc740\ube44\ub294 \ubc99\ucee4\ub97c \ub098\uc11c\uba70 \ube44\uc5d0 \uc816\uc740 \ubd89\uc740 \ud6c4\ub4dc\ub97c \ud138\uc5b4\ub0c8\ub2e4. \ub3c4\uc2dc\ub294 \uc5ec\uc804\ud788 \uc5b4\ub461\uace0 \ube44\uac00 \ub0b4\ub838\uc9c0\ub9cc, \uc624\ub298 \ubc24 \uc0ac\ub0e5\uafbc\uc740 \ub291\ub300\uac00 \uc544\ub2c8\ub77c \ubd89\uc740 \ub450\uac74\uc774\uc5c8\ub2e4.\n\n\"\ub2e4\uc74c \uc758\ub8b0\ub294 \ubb50\uc8e0?\"\n\uadf8\ub140\ub294 \ub124\uc628 \uc0ac\uc778 \uc18d\uc73c\ub85c \ub2e4\uc2dc \uc0ac\ub77c\uc84c\ub2e4.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 337, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "b4d93ac5-d3d5-40d4-837f-b9b03ac2d1bf": {"__data__": {"id_": "b4d93ac5-d3d5-40d4-837f-b9b03ac2d1bf", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "8a446e75-beb4-4d83-a0b8-91fb93bd85d8", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "1912194b7fd4941185ff887e4ecbd21b6f947ce0d9c65206ec25bae5d63c34d2", "class_name": "RelatedNodeInfo"}, "3": {"node_id": "45a7f998-bdc1-47b9-b9c6-fdac842a2d96", "node_type": "1", "metadata": {}, "hash": "62043bbb608dd4e272cd26800d3ec618f215f4bb01a38dfef789df207f93de7f", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "import os\n\n# \uc774\uc57c\uae30 \ub370\uc774\ud130 (\uac01 \uc7a5\uc758 \uc81c\ubaa9\uacfc \ub0b4\uc6a9)\nstory_data = {\n    \"redhood1\": \"\"\"2099\ub144 \ub124\uc624-\uc11c\uc6b8, \uc0b0\uc131\ube44\uac00 \ub04a\uc784\uc5c6\uc774 \ub0b4\ub9ac\ub294 \ud558\uce35 \uad6c\uc5ed. \uc815\ubcf4 \ubc30\ub2ec\ubd80 '\uc740\ube44'\ub294 \ub0a1\uc740 \uc544\ud30c\ud2b8 \uc625\uc0c1\uc5d0\uc11c \uc758\ub8b0\uc778\uc744 \ub9cc\ub0ac\ub2e4. \uadf8\ub140\uc758 \ud2b8\ub808\uc774\ub4dc\ub9c8\ud06c\ub294 \uad11\ud559 \ubbf8\ucc44 \uae30\ub2a5\uc774 \ud0d1\uc7ac\ub41c \ubd89\uc740\uc0c9 \ud640\ub85c\uadf8\ub7a8 \ud6c4\ub4dc\uc600\ub2e4.\n\n\"\uc774\uac78 '\ud560\uba38\ub2c8'\uc5d0\uac8c \uc804\ud574\ub77c.\"\n\uc758\ub8b0\uc778\uc740 \ub5a8\ub9ac\ub294 \uc190\uc73c\ub85c \ubd89\uac8c \ube5b\ub098\ub294 \uc0dd\uccb4 \ub370\uc774\ud130 \uce69\uc744 \uac74\ub138\ub2e4. \uadf8 \uc548\uc5d0\ub294 \uba54\uac00\ucf54\ud504(\uac70\ub300 \uae30\uc5c5) '\uc6b8\ud504 \uc0ac'\uc758 \ubd88\ubc95 \uc0dd\uccb4 \uc2e4\ud5d8 \uc99d\uac70\uac00 \ub4e4\uc5b4 \uc788\uc5c8\ub2e4. '\ud560\uba38\ub2c8'\ub294 \uad6c\uc2dc\uac00\uc9c0 \uc9c0\ud558 \ubc99\ucee4\uc5d0 \uc228\uc5b4 \uc9c0\ub0b4\ub294 \uc804\uc124\uc801\uc778 \ud574\ucee4\uc758 \ucf54\ub4dc\ub124\uc784\uc774\uc5c8\ub2e4.\n\n\"\uacbd\ub85c\ub97c \uc870\uc2ec\ud574. \ub188\ub4e4\uc758 '\uc0ac\ub0e5\uac1c'\ub4e4\uc774 \ub0c4\uc0c8\ub97c \ub9e1\uc558\uc5b4.\"\n\uc740\ube44\ub294 \ubd89\uc740 \ud6c4\ub4dc\ub97c \uba38\ub9ac \uc704\ub85c \ub20c\ub7ec\uc4f0\uba70 \ube57\uc18d\uc73c\ub85c \uc0ac\ub77c\uc84c\ub2e4. \uadf8\ub140\uc758 \ub9dd\ub9c9 \ub514\uc2a4\ud50c\ub808\uc774\uc5d0 \ubaa9\uc801\uc9c0\uae4c\uc9c0\uc758 \uacbd\ub85c\uac00 \ubd89\uc740 \uc2e4\uc120\uc73c\ub85c \ud45c\uc2dc\ub418\uc5c8\ub2e4.\"\"\",\n\n    \"redhood2\": \"\"\"\ub3c4\uc2dc\uc758 \ub4b7\uace8\ubaa9\uc740 \uac70\ub300\ud55c \ub124\uc628\uc0ac\uc778\uc774 \ub9cc\ub4e4\uc5b4\ub0b8 \uc778\uacf5\uc801\uc778 \uc232\uc774\uc5c8\ub2e4. \uc740\ube44\ub294 \ub4dc\ub860 \uac10\uc2dc\ub9dd\uc744 \ud53c\ud574 \uc2ac\ub7fc\uac00\uc758 \ubcf5\uc7a1\ud55c \ud30c\uc774\ud504\ub77c\uc778 \uc704\ub97c \ub2ec\ub838\ub2e4. \ubc14\ub2e5\uc740 \uc816\uc5b4 \uc788\uc5c8\uace0, \uacf5\uae30\ub294 \ub9e4\uce90\ud55c \uc804\uc790 \ub2f4\ubc30 \uc5f0\uae30\uc640 \uc624\uc874 \ub0c4\uc0c8\ub85c \uac00\ub4dd\ud588\ub2e4.\n\n\uadf8\ub54c, \uc740\ube44\uc758 \uccad\uac01 \uc13c\uc11c\uc5d0 \ubbf8\uc138\ud55c \ub178\uc774\uc988\uac00 \uc7a1\ud614\ub2e4. \uc77c\ubc18\uc801\uc778 \uae30\uacc4 \uc18c\uc74c\uc774 \uc544\ub2c8\uc5c8\ub2e4. \ub204\uad70\uac00 \uace0\uc131\ub2a5 \uc2a4\ud154\uc2a4 \ubaa8\ub4c8\uc744 \ucf1c\uace0 \uadf8\ub140\ub97c \ucd94\uc801\ud558\uace0 \uc788\uc5c8\ub2e4. \uc740\ube44\ub294 \ubcf8\ub2a5\uc801\uc73c\ub85c \uacbd\ub85c\ub97c \uc218\uc815\ud574 \ub354 \uae4a\uace0 \uc5b4\ub450\uc6b4 '\ud3d0\uae30\ubb3c \ucc98\ub9ac \uad6c\uc5ed'\uc73c\ub85c \ubc29\ud5a5\uc744 \ud2c0\uc5c8\ub2e4. \uc232\uc18d\uc758 \uc624\uc194\uae38 \ub300\uc2e0, \uadf8\ub140\ub294 \ub370\uc774\ud130 \uc4f0\ub808\uae30\uac00 \ud750\ub974\ub294 \ud558\uc218\ub3c4\ub97c \ud0dd\ud588\ub2e4.\"\"\",\n\n    \"redhood3\": \"\"\"\ud3d0\uae30\ubb3c \uad6c\uc5ed\uc758 \ucd9c\uad6c, \ud55c \ub0a8\uc790\uac00 \uc740\ube44\uc758 \uae38\uc744 \ub9c9\uc544\uc130\ub2e4. \ud68c\uc0c9 \ud2b8\ub80c\uce58\ucf54\ud2b8\ub97c \uc785\uc740 \uadf8\ub294 \uac89\ubcf4\uae30\uc5d4 \ud3c9\ubc94\ud574 \ubcf4\uc600\uc9c0\ub9cc, \uc740\ube44\uc758 \uc2a4\uce90\ub108\ub294 \uadf8\uc758 \ubab8 90%\uac00 \uad70\uc6a9 \ud06c\ub86c \uc758\uccb4\uc784\uc744 \uc54c\ub9ac\uace0 \uc788\uc5c8\ub2e4. \ucf54\ub4dc\ub124\uc784 '\uc6b8\ud504'. \uc6b8\ud504 \uc0ac\uc758 \ucd5c\uace0 \ud574\uacb0\uc0ac\uc600\ub2e4.\n\"\uaf2c\ub9c8 \uc544\uac00\uc528, \ubb34\uac70\uc6b4 \uc9d0\uc744 \ub4e4\uace0 \uac00\ub294\uad70. \ub0b4\uac00 \ub3c4\uc640\uc904\uae4c?\"\n\ub0a8\uc790\uc758 \ubaa9\uc18c\ub9ac\ub294 \uae01\ud788\ub294 \ub4ef\ud55c \uae30\uacc4\uc74c\uc774 \uc11e\uc5ec \uc788\uc5c8\ub2e4. \uadf8\ub294 \uc740\ube44\uac00 \uac00\uc9c4 \uce69\uc774 \ubb34\uc5c7\uc778\uc9c0 \uc815\ud655\ud788 \uc54c\uace0 \uc788\uc5c8\ub2e4.\n\"\uc9c0\ub984\uae38\uc744 \uc54c\ub824\uc8fc\uc9c0.", "mimetype": "text/plain", "start_char_idx": 0, "end_char_idx": 996, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "45a7f998-bdc1-47b9-b9c6-fdac842a2d96": {"__data__": {"id_": "45a7f998-bdc1-47b9-b9c6-fdac842a2d96", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "8a446e75-beb4-4d83-a0b8-91fb93bd85d8", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "1912194b7fd4941185ff887e4ecbd21b6f947ce0d9c65206ec25bae5d63c34d2", "class_name": "RelatedNodeInfo"}, "2": {"node_id": "b4d93ac5-d3d5-40d4-837f-b9b03ac2d1bf", "node_type": "1", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "f4d6837fb4dac38be09d9fd2a93ac56c1d0e70d5c2ff33a7050cd47054b48ba9", "class_name": "RelatedNodeInfo"}, "3": {"node_id": "e694d538-593d-4a51-8bc3-a0a2cde1958d", "node_type": "1", "metadata": {}, "hash": "287e9fdf9cbfdf9a2f1f77791b49a14f330d011be2b023ab2644acb6ece94ebc", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\ucf54\ub4dc\ub124\uc784 '\uc6b8\ud504'. \uc6b8\ud504 \uc0ac\uc758 \ucd5c\uace0 \ud574\uacb0\uc0ac\uc600\ub2e4.\n\"\uaf2c\ub9c8 \uc544\uac00\uc528, \ubb34\uac70\uc6b4 \uc9d0\uc744 \ub4e4\uace0 \uac00\ub294\uad70. \ub0b4\uac00 \ub3c4\uc640\uc904\uae4c?\"\n\ub0a8\uc790\uc758 \ubaa9\uc18c\ub9ac\ub294 \uae01\ud788\ub294 \ub4ef\ud55c \uae30\uacc4\uc74c\uc774 \uc11e\uc5ec \uc788\uc5c8\ub2e4. \uadf8\ub294 \uc740\ube44\uac00 \uac00\uc9c4 \uce69\uc774 \ubb34\uc5c7\uc778\uc9c0 \uc815\ud655\ud788 \uc54c\uace0 \uc788\uc5c8\ub2e4.\n\"\uc9c0\ub984\uae38\uc744 \uc54c\ub824\uc8fc\uc9c0. \uadf8 \uce69\ub9cc \ub118\uae34\ub2e4\uba74 \uc548\uc804\ud558\uac8c \uc9d1\uc5d0 \uac08 \uc218 \uc788\uc5b4.\"\n\n\uc740\ube44\ub294 \ub300\ub2f5 \ub300\uc2e0 \uc8fc\uba38\ub2c8 \uc18d\uc758 EMP \uc218\ub958\ud0c4\uc744 \uaf49 \uc950\uc5c8\ub2e4. \"\ub09c \ub0af\uc120 \uc0ac\ub78c\uacfc\ub294 \ub370\uc774\ud130 \ud1b5\uc2e0\uc744 \ud558\uc9c0 \uc54a\uc544\uc694.\" \"\"\",\n\n    \"redhood4\": \"\"\"\ud3ed\ubc1c\uc74c\uacfc \ud568\uaed8 \uc740\ube44\ub294 \uc9c0\uc0c1\uc73c\ub85c \ud280\uc5b4 \uc62c\ub790\ub2e4. EMP\ub294 \uc6b8\ud504\ub97c \uc7a0\uc2dc \ub9c8\ube44\uc2dc\ucf30\uc9c0\ub9cc, \uadf8\ub294 \uae08\uc138 \uc2dc\uc2a4\ud15c\uc744 \uc7ac\ubd80\ud305\ud558\uace0 \uc9d0\uc2b9 \uac19\uc740 \uc18d\ub3c4\ub85c \ucd94\uaca9\uc744 \uc2dc\uc791\ud588\ub2e4. \uadf8\uc758 \ud314\uc774 \ubcc0\ud615\ub418\uc5b4 \ub0a0\uce74\ub85c\uc6b4 \ubaa8\ub178-\uc640\uc774\uc5b4 \ud074\ub85c(\ubc1c\ud1b1)\uac00 \ud280\uc5b4 \ub098\uc654\ub2e4.\n\n\uc740\ube44\ub294 \ub9c8\uadf8\ub124\ud2f1 \ubd80\uce20\ub97c \uc791\ub3d9\uc2dc\ucf1c \uac74\ubb3c\uc758 \uc218\uc9c1 \ubcbd\uc744 \ub2ec\ub838\ub2e4. \ubd89\uc740 \ud6c4\ub4dc\uc758 \uad11\ud559 \ubbf8\ucc44\ub97c \ud65c\uc131\ud654\ud574 \uc8fc\ubcc0\uc758 \ub124\uc628 \uc0ac\uc778\uacfc \ub3d9\ud654\ub418\ub824 \ud588\uc9c0\ub9cc, \uc6b8\ud504\uc758 \uc5f4 \uac10\uc9c0 \uc13c\uc11c\ub294 \uadf8\ub140\ub97c \ub193\uce58\uc9c0 \uc54a\uc558\ub2e4. \ub3c4\uc2dc\uc758 \ub9c8\ucc9c\ub8e8 \uc0ac\uc774\ub97c \uc624\uac00\ub294 \uc228 \ub9c9\ud788\ub294 \ucd94\uaca9\uc804\uc774 \ubc8c\uc5b4\uc84c\ub2e4. \uc740\ube44\ub294 \ud574\ud0b9 \ud234\uc744 \uc774\uc6a9\ud574 \uc2e0\ud638\ub4f1\uc744 \uc870\uc791\ud558\uace0 \uad11\uace0\ud310\uc744 \ub5a8\uc5b4\ub728\ub9ac\uba70 \uac04\uc2e0\ud788 \uc6b8\ud504\uc640\uc758 \uac70\ub9ac\ub97c \ubc8c\ub838\ub2e4.\"\"\",\n\n    \"redhood5\": \"\"\"\uad6c\uc2dc\uac00\uc9c0\uc758 \ud3d0\ud5c8, \uc78a\ud600\uc9c4 \uc9c0\ud558\ucca0\uc5ed \uae4a\uc740 \uacf3\uc5d0 '\ud560\uba38\ub2c8'\uc758 \ubc99\ucee4\uac00 \uc788\uc5c8\ub2e4. \uc740\ube44\ub294 \uc228\uc744 \ud5d0\ub5a1\uc774\uba70 \uc721\uc911\ud55c \uac15\ucca0 \ubb38\uc744 \ud574\ud0b9\ud574 \uc5f4\uc5c8\ub2e4.\n\"\ud560\uba38\ub2c8! \ub370\uc774\ud130 \uac00\uc838\uc654\uc5b4\uc694!\"\n\n\ud558\uc9c0\ub9cc \ubc99\ucee4 \uc548\uc740 \ucc28\uac11\uac8c \uc2dd\uc5b4 \uc788\uc5c8\ub2e4. \uc218\ub9ce\uc740 \uc11c\ubc84\uac00 \uc6c5\uc6c5\uac70\ub9ac\ub294 \uc18c\ub9ac \ub300\uc2e0, \uae30\uad34\ud55c \uc815\uc801\ub9cc\uc774 \uac10\ub3cc\uc558\ub2e4. \uc911\uc559 \uc81c\uc5b4 \uc758\uc790\uc5d0\ub294 \ub1cc\uc640 \uc11c\ubc84\uac00 \uc9c1\uc811 \uc5f0\uacb0\ub41c \ub178\ud30c\uac00 \uc549\uc544 \uc788\uc5c8\uc9c0\ub9cc, \uadf8\ub140\uc758 \ubaa8\ub2c8\ud130\uc5d0\ub294 \ubd89\uc740\uc0c9 'ACCESS DENIED(\uc811\uadfc \uac70\ubd80)' \uba54\uc2dc\uc9c0\ub9cc\uc774 \uae5c\ube61\uc774\uace0 \uc788\uc5c8\ub2e4.\n\n\"\ub2a6\uc5c8\uad6c\ub098, \uc544\uac00\uc528.\"\n\uc5b4\ub460 \uc18d\uc5d0\uc11c \ud68c\uc0c9 \ucf54\ud2b8\uc758 \ub0a8\uc790\uac00 \uac78\uc5b4 \ub098\uc654\ub2e4. \uadf8\ub294 \uc740\ube44\ubcf4\ub2e4 \uba3c\uc800 \ub3c4\ucc29\ud574 '\ud560\uba38\ub2c8'\uc758 \uc2dc\uc2a4\ud15c\uc744 \uc7a5\uc545\ud558\uace0, \uc790\uc2e0\uc758 \ubaa9\uc18c\ub9ac\ub97c \ud560\uba38\ub2c8\uc758 \uc74c\uc131 \ud569\uc131\uae30\ub85c \ubcc0\uc870\ud574 \uc740\ube44\ub97c \uc720\uc778\ud588\ub358 \uac83\uc774\ub2e4.\"\"\",\n\n    \"redhood6\": \"\"\" \"\uadf8 \uce69\uc744 \ub0b4\ub193\uc73c\uc2dc\uc9c0.\"\n\uc6b8\ud504\uac00 \ubc1c\ud1b1\uc744 \uc138\uc6b0\uba70 \ub2e4\uac00\uc654\ub2e4. \uc740\ube44\ub294 \ub9c9\ub2e4\ub978 \uacf3\uc5d0 \ubab0\ub838\ub2e4.", "mimetype": "text/plain", "start_char_idx": 865, "end_char_idx": 1867, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "e694d538-593d-4a51-8bc3-a0a2cde1958d": {"__data__": {"id_": "e694d538-593d-4a51-8bc3-a0a2cde1958d", "embedding": null, "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "8a446e75-beb4-4d83-a0b8-91fb93bd85d8", "node_type": "4", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "1912194b7fd4941185ff887e4ecbd21b6f947ce0d9c65206ec25bae5d63c34d2", "class_name": "RelatedNodeInfo"}, "2": {"node_id": "45a7f998-bdc1-47b9-b9c6-fdac842a2d96", "node_type": "1", "metadata": {"file_path": "/Users/boseung/ai-engineering/gemini-programming/07/data/story_gen.py", "file_name": "story_gen.py", "file_type": "text/x-python", "file_size": 6127, "creation_date": "2025-12-28", "last_modified_date": "2025-12-28"}, "hash": "bfa0ead044f3592dd16057f492b5d19dc3a60c4921301210d1ace8e8678345ba", "class_name": "RelatedNodeInfo"}}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "\"\ub2a6\uc5c8\uad6c\ub098, \uc544\uac00\uc528.\"\n\uc5b4\ub460 \uc18d\uc5d0\uc11c \ud68c\uc0c9 \ucf54\ud2b8\uc758 \ub0a8\uc790\uac00 \uac78\uc5b4 \ub098\uc654\ub2e4. \uadf8\ub294 \uc740\ube44\ubcf4\ub2e4 \uba3c\uc800 \ub3c4\ucc29\ud574 '\ud560\uba38\ub2c8'\uc758 \uc2dc\uc2a4\ud15c\uc744 \uc7a5\uc545\ud558\uace0, \uc790\uc2e0\uc758 \ubaa9\uc18c\ub9ac\ub97c \ud560\uba38\ub2c8\uc758 \uc74c\uc131 \ud569\uc131\uae30\ub85c \ubcc0\uc870\ud574 \uc740\ube44\ub97c \uc720\uc778\ud588\ub358 \uac83\uc774\ub2e4.\"\"\",\n\n    \"redhood6\": \"\"\" \"\uadf8 \uce69\uc744 \ub0b4\ub193\uc73c\uc2dc\uc9c0.\"\n\uc6b8\ud504\uac00 \ubc1c\ud1b1\uc744 \uc138\uc6b0\uba70 \ub2e4\uac00\uc654\ub2e4. \uc740\ube44\ub294 \ub9c9\ub2e4\ub978 \uacf3\uc5d0 \ubab0\ub838\ub2e4. \ubb3c\ub9ac\uc801\uc778 \ud798\uc73c\ub85c\ub294 \uadf8\ub97c \uc774\uae38 \uc218 \uc5c6\uc5c8\ub2e4. \uc740\ube44\ub294 \uce69\uc744 \uac74\ub124\uc8fc\ub294 \ucc99\ud558\uba70 \uc2ac\ub86f\uc5d0 \uce69\uc744 \uaf42\uc558\ub2e4.\n\n\"\uba4d\uccad\ud558\uad70.\" \uc6b8\ud504\uac00 \ube44\uc6c3\uc73c\uba70 \uce69\uc758 \ub370\uc774\ud130\ub97c \uc790\uc2e0\uc758 \uc11c\ubc84\ub85c \uc804\uc1a1\ud558\ub824 \ud588\ub2e4.\n\ud558\uc9c0\ub9cc \uadf8 \uc21c\uac04, \ubc99\ucee4\uc758 \ubaa8\ub4e0 \ud654\uba74\uc774 \ubd89\uac8c \ubcc0\ud588\ub2e4. \uc740\ube44\uac00 \uac00\uc838\uc628 \uac83\uc740 \ub2e8\uc21c\ud55c \uc99d\uac70 \ub370\uc774\ud130\uac00 \uc544\ub2c8\uc5c8\ub2e4. \uadf8\uac83\uc740 \ud560\uba38\ub2c8\uac00 \uc0ac\uc804\uc5d0 \uc2ec\uc5b4\ub454 \uc790\uc728 \ub300\uc751 \ubc14\uc774\ub7ec\uc2a4, '\ub354 \ud5cc\ud130(The Hunter)'\uc758 \uc2e4\ud589\ud0a4\uc600\ub2e4.\n\n\"\ubb50, \ubb50\uc9c0? \ub0b4 \uc2dc\uc2a4\ud15c\uc774...!\"\n\uc6b8\ud504\uc758 \uc758\uccb4\uac00 \uacbd\ub828\uc744 \uc77c\uc73c\ucf30\ub2e4. \ubc14\uc774\ub7ec\uc2a4\ub294 \uc678\ubd80 \uc11c\ubc84\uac00 \uc544\ub2c8\ub77c, \uc811\uc18d\ub41c \uc6b8\ud504\uc758 \ub1cc \uc2e0\uacbd\ub9dd\uc744 \uc9c1\uc811 \uacf5\uaca9\ud558\uae30 \uc2dc\uc791\ud588\ub2e4.\"\"\",\n\n    \"redhood7\": \"\"\"\uc6b8\ud504\ub294 \uacfc\ubd80\ud558\ub85c \uc778\ud574 \uc5f0\uae30\ub97c \ub0b4\ubfdc\uc73c\uba70 \uc4f0\ub7ec\uc84c\ub2e4. \uadf8\uc758 \uc2dc\uc57c\uac00 \uaebc\uc9c0\uae30 \uc804, \ub9c8\uc9c0\ub9c9\uc73c\ub85c \ubcf8 \uac83\uc740 \ubd89\uc740 \ud6c4\ub4dc\ub97c \uc4f4 \uc740\ube44\uac00 \ucc28\uac11\uac8c \uadf8\ub97c \ub0b4\ub824\ub2e4\ubcf4\ub294 \ubaa8\uc2b5\uc774\uc5c8\ub2e4.\n\n\uc740\ube44\ub294 \ud560\uba38\ub2c8\uc758 \uba54\uc778 \uc11c\ubc84\ub97c \ubcf5\uad6c\ud558\uace0, \uae30\uc5c5\uc758 \ube44\ub9ac \ub370\uc774\ud130\ub97c \uc804 \ub3c4\uc2dc\uc758 \ud640\ub85c\uadf8\ub7a8 \uad11\uace0\ud310\uc73c\ub85c \uc1a1\ucd9c\ud588\ub2e4. \ub124\uc624-\uc11c\uc6b8\uc758 \ubc24\ud558\ub298\uc774 \uc9c4\uc2e4\uc744 \uc54c\ub9ac\ub294 \ubd89\uc740 \ube5b\uc73c\ub85c \ubb3c\ub4e4\uc5c8\ub2e4.\n\n\"\uc218\uace0\ud588\ub2e4, \uc740\ube44\uc57c.\" \uc2a4\ud53c\ucee4\ub97c \ud1b5\ud574 \ubcf5\uad6c\ub41c \ud560\uba38\ub2c8\uc758 AI \uc74c\uc131\uc774 \ud758\ub7ec\ub098\uc654\ub2e4.\n\uc740\ube44\ub294 \ubc99\ucee4\ub97c \ub098\uc11c\uba70 \ube44\uc5d0 \uc816\uc740 \ubd89\uc740 \ud6c4\ub4dc\ub97c \ud138\uc5b4\ub0c8\ub2e4. \ub3c4\uc2dc\ub294 \uc5ec\uc804\ud788 \uc5b4\ub461\uace0 \ube44\uac00 \ub0b4\ub838\uc9c0\ub9cc, \uc624\ub298 \ubc24 \uc0ac\ub0e5\uafbc\uc740 \ub291\ub300\uac00 \uc544\ub2c8\ub77c \ubd89\uc740 \ub450\uac74\uc774\uc5c8\ub2e4.\n\n\"\ub2e4\uc74c \uc758\ub8b0\ub294 \ubb50\uc8e0?\"\n\uadf8\ub140\ub294 \ub124\uc628 \uc0ac\uc778 \uc18d\uc73c\ub85c \ub2e4\uc2dc \uc0ac\ub77c\uc84c\ub2e4.\"\"\"\n}\n\n# \ud30c\uc77c \uc0dd\uc131 \ud568\uc218\ndef create_story_files():\n    for title, content in story_data.items():\n        filename = f\"{title}.txt\"\n        try:\n            with open(filename, \"w\", encoding=\"utf-8\") as f:\n                f.write(content)\n            print(f\"\uc0dd\uc131 \uc644\ub8cc: {filename}\")\n        except Exception as e:\n            print(f\"\uc624\ub958 \ubc1c\uc0dd ({filename}): {e}\")\n\nif __name__ == \"__main__\":\n    create_story_files()", "mimetype": "text/plain", "start_char_idx": 1683, "end_char_idx": 2923, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}}}